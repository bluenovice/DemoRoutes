<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="Route.demoRoute.controller.SecondView"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title">
				<content>
					<Label text="{ProductName}"></Label>
					<Label text="{ProductID}"></Label>
					<Label text="{UnitPrice}"></Label>
					<Link text="Click to get Detail View" press=".onDetailPage"></Link>
				</content>
				<Button text="Back" press="onPress"/>
			</Page>
		</pages>
	</App>
</mvc:View>